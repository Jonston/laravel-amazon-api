{
    "name": "jonston/amazon-ads",
    "description": "Amazon Ads API low-level Laravel package",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jonston\\AmazonAds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonston\\AmazonAds\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jonston\\AmazonAds\\AmazonAdsServiceProvider"
            ],
            "aliases": {
                "AmazonAds": "Jonston\\AmazonAds\\Facades\\AmazonAds"
            }
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
