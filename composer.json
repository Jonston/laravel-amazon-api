{
    "name": "jonston/amazon-ads-api",
    "description": "Laravel package for Amazon Advertising API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jonston\\AmazonAdsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonston\\AmazonAdsApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jonston\\AmazonAdsApi\\AmazonApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
